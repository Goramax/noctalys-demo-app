{
    "name": "noctalys/demo-app",
    "description": "Noctalys Demo Application",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DemoApp\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../NoctalysFramework",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "noctalys/framework": "^0.1.0"
    },
    "suggest": {
        "twig/twig": "Required to use Twig templates",
        "smarty/smarty": "Required to use Smarty templates",
        "latte/latte": "Required to use Latte templates"
    }
}
